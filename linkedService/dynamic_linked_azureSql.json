{
	"name": "dynamic_linked_azureSql",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"ServerName": {
				"type": "string"
			},
			"UserId": {
				"type": "string"
			},
			"Password": {
				"type": "string"
			},
			"DatabaseName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().ServerName};Initial Catalog=@{linkedService().DatabaseName};User ID=@{linkedService().UserId};Password=@{linkedService().Password}"
		}
	}
}